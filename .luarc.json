{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "diagnostics": {
    "disable": [
      "duplicate-doc-field",
      "duplicate-doc-alias",
      "missing-fields",
      "redundant-return-value"
    ]
  },
  "runtime": {
    "version": "Lua 5.4",
    "path": [
      "?.lua",
      "${workspace}/PudimServer/init.lua",
      "${workspace}/PudimServer/?.lua"
    ]
  },
  "workspace": {
    "library": [
      "library"
    ],
    "ignoreDir": [
      "lua_modules",
      "rockspecs",
      ".vscode",
      "spec"
    ]
  }
}
